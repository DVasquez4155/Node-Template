<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel='stylesheet' href="/assets/css/style.css">
        <link href="/css/bootstrap.min.css" rel="stylesheet">
        <script src="/js/jquery.js"></script>
        <script src="/js/bootstrap.min.js"></script>
        {{#each script}}
            <script type="text/javascript" src="assets/js/{{this}}"></script>
        {{/each}}
        <title>Project - {{title}}</title>
    </head>
    <body>
        <nav class="navbar navbar-dark navbar-expand-lg bg-dark ">
            <div class="container"><a class="navbar-brand logo" href="/">Project</a><button data-toggle="collapse" class="navbar-toggler" data-target="#navbarNav"><span class="sr-only">Toggle navigation</span><span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="nav navbar-nav ml-auto">
                        {{#each link}}
                            <li class="nav-item" role="presentation"><a class="nav-link" href="{{link}}">{{text}}</a></li>
                        {{/each}}
                    </ul>
                </div>
            </div>
        </nav>
        <br>
        <main class="page">
            <div class="container">
                {{{ body }}}
            </div>
        </main>
    </body>
</html>